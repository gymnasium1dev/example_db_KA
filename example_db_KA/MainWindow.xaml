<Window x:Class="example_db_KA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:example_db_KA"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="addTipes" Click="GetTipes_Click" Content="Click"></Button>
        <Button x:Name="PrintTypes" Click="PrintTypes_Click" Content="click"></Button>
        <StackPanel Orientation="Horizontal">
            <TextBox x:Name="TypeTitle"></TextBox>
            <Button x:Name="addType" Content="addType" Click="addType_Click"></Button>
        </StackPanel>
    </StackPanel>
</Window>
